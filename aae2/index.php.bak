	<script>
		var pi = <?=$zoom_inc_prc?> ;
		var ww, ot  ;
		window.onbeforeunload = function () { window.scrollTo(0, 0);}
		$(function() {
			ww = $(window).width();
			ot = $('#mark').offset().top ;
			var p = 100 + pi ;
			$('#f0').css({'margin-left' : '-' + $('#f0').width()/2 + 'px'});
			$('#f0').css({'margin-top' : (ot*pi/100)/2 + 'px'});
			//alert(window.innerHeight); 
		});
		$(window).on('resize orientationchange', function() {
			//$('#dsp1').text($(window).width());
			//$('#dsp2').text(ww);
			if ($(window).width() != ww)
				location.href = location.href ;
			//$('#f11').css({'height' : $(this).innerHeight()/12 + 'px'});
			//alert(window.innerHeight);
			//$('#f10').hide();
			//$('.fixed').css({'height' : window.innerHeight + 'px'})
		});
		$(window).scroll(function() {
			//$('#dsp').text($(window).height());
			var scrollTop = $(window).scrollTop();
			var n = Math.floor(scrollTop/ot);
			var p = 100 + pi - pi*(scrollTop - n*ot)/ot ;
			if (p < 100) p = 100 ;
			$('#f' + n).css({'width' : p + '%', 'height' : p + '%'});
			var j = ot*0.21 ;
			var k = ot*0.05 ;
			var pp = (scrollTop - n*ot)/ot ;
			var pd = pp - 0.75*(1 - pp);
			if (pd < 0)
				pd = 0 ;
			$('#cpt' + n).css({'top' : j - k*pd + 'px', 'opacity' : pd});
			if (n == 12)
				p = 100 ;
			var mt = (ot*p/100)/2 ;
			$('#f' + n).css({'margin-top' : '-' + mt + 'px'});
			$('#f' + n).css({'margin-left' : '-' + $('#f' + n).width()/2 + 'px'});
			var l = n + 1 ;
			while ($('#cpt' + l).length) {
				$('#cpt' + l).css({'opacity' : 0});
				l ++ ;
			}
			if (n > 0) {
				var pd = pp + 0.75*(1 - pp);
				if (pd > 1)
					pd = 1 ;
				
				$('#cpt' + (n - 1)).css({'top' : j - k - k*pp*1.8 + 'px', 'opacity' : 1 - pp*1.8});
				l = n - 2 ;
				while ($('#cpt' + l).length) {
					$('#cpt' + l).css({'opacity' : 0});
					l -- ;	
				}
			}
			while (n > 0) {
				var i = n - 1 ;
				var p = 125*(scrollTop - n*ot)/ot ;
				if (i == 10)
					$('#dsp').text(p);
				if (i == 10) {
					var isAndroid = /(android)/i.test(navigator.userAgent);
					if (p >= 100) {
						$('#f10').hide();
						if (isAndroid)
							$('#android-toolbal').show();
					}
					else {
						$('#f10').show()
						if (isAndroid)
							$('#android-toolbal').hide();
					}
				}
				$('#f' + i).css({'margin-top' : '-' + (ot/2 + ot*p/100) + 'px'});
				n = i ;
			}
		});
	</script>
